	my ($clusterInfoHsh_ref) = @_;
	
	&reportAndLogStatus("removing unanno cluster within filter region", 10, "\n");#->2455
	my $num_rm = 0;
	my $num_unanno = 0;
	foreach my $clusterID (keys %{$clusterInfoHsh_ref}) {
