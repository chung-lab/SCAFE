sub printTrnscptGtf {
#....................................................................................................................................................#
#	subroutineCategory: specific
#	dependOnSub: exonRngToGtf|739
#	appearInSub: >none
#	primaryAppearInSection: 3_readInfo|184
#	secondaryAppearInSection: >none
#	input: $geneInfoHsh_ref, $outputPrefix, $result_gtf_dir, $trnscptInfoHsh_ref
