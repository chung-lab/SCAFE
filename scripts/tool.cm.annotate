		open (FILEIN, "<", $gene_info_path);
	}

	chomp(my $header = <FILEIN>);

	my %geneNameHsh = ();
	my @headerSplt = split /\t/, $header;
	my %colIndexHsh = ();
	
	foreach my $headerItem (qw/geneID geneName geneClass/) {
		$colIndexHsh{$headerItem} = undef;
		foreach my $i (0..$#headerSplt) {
