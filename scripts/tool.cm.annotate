	my ($result_gtf_dir, $trnscptInfoHsh_ref, $geneInfoHsh_ref, $outputPrefix) = @_;
	
	my $all_trnscpt_gtf_path = "$result_gtf_dir/$outputPrefix.gtf.gz";
