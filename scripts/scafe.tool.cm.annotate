				$IDHsh{'geneID'}{$geneID}++;
				$IDHsh{'trnscptID'}{$trnscptID}++;
				$IDHsh{'geneName'}{$geneName}++;
				$IDHsh{'geneClass'}{$geneClass}++;
				$IDHsh{'codingClass'}{$codingClass}++;
