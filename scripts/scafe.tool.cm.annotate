			}
		
			$geneInfoHsh_ref->{$geneID}{'geneName'} = $geneName;
			$geneInfoHsh_ref->{$geneID}{'geneClass'} = $geneClass;
			$geneNameHsh{$geneName}++;
		}
	}
	close FILEIN;
	
	my $num_gene = keys %{$geneInfoHsh_ref};
	&reportAndLogStatus("$num_gene genes reads", 10, "\n");#->2953
	
	return ();
}
