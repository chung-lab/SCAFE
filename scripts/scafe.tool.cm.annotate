				$colIndexHsh{$headerItem} = $i if $headerSplt[$i] eq 'geneType';
			}
		}
		
		die if not defined $colIndexHsh{$headerItem};
	}

	while (<FILEIN>) {
		next if $_ =~ m/^#/;
		chomp;
		my @splt = split /\t/;
		my $geneID = $splt[$colIndexHsh{'geneID'}];
		my $geneName = $splt[$colIndexHsh{'geneName'}];
